{
  "manifest_version": 3,
  "name": "Google Meets Ping Backoff",
  "version": "0.0.1",
  "description": "When a listed known Google Chat spammer sends a message. Mute the tab for 30 seconds with an incremental back-off",
  "background": {
    "service_worker": "background.js"
  },
  "author": "Tim Hiatt - timbohiatt@gmail.com",
  "permissions": [
    "scripting",
    "activeTab",
    "contextMenus",
    "tabs",
    "notifications",
    "https://mail.google.com/*"
  ],
  "host_permissions": [
    "https://mail.google.com/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "key": ""
}